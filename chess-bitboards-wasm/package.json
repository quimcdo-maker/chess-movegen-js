{
    "name": "chess-bitboards-wasm",
    "version": "0.1.0",
    "description": "High-performance chess bitboard operations in WebAssembly",
    "main": "node/chess_bitboards_wasm.js",
    "module": "bundler/chess_bitboards_wasm.js",
    "types": "bundler/chess_bitboards_wasm.d.ts",
    "files": [
        "web/",
        "node/",
        "bundler/"
    ],
    "scripts": {
        "test": "wasm-pack test --target web",
        "build": "bash build.sh",
        "build:web": "wasm-pack build --target web --out-dir pkg/web --release",
        "build:node": "wasm-pack build --target node --out-dir pkg/node --release",
        "build:bundler": "wasm-pack build --target bundler --out-dir pkg/bundler --release",
        "serve": "python -m http.server 8080 --directory pkg/web",
        "demo": "python -m http.server 8080 --directory ."
    },
    "keywords": [
        "chess",
        "bitboards",
        "webassembly",
        "rust",
        "performance"
    ],
    "author": "Mario Carbonell",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/mcarbonell/chess-movegen-js.git"
    },
    "dependencies": {},
    "devDependencies": {}
}